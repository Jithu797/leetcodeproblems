class Solution {
    public int findMaxConsecutiveOnes(int[] nums) {
        int maxi=0;
        int c=0;
        for (int i=0;i<nums.length;i++){
            if(nums[i]==1){
                c=c+1;
            }
            else{
                c=0;
            }
            maxi=Math.max(maxi,c);
            }
        return maxi;
        
    }
}
